{
  "name": "eslint-config-prettier",
  "version": "1.0.3",
  "license": "MIT",
  "author": "Simon Lydell",
  "description": "Turns all rules that are unnecessary or might conflict with prettier off.",
  "repository": "lydell/eslint-config-prettier",
  "files": [
    "cli.js",
    "index.js",
    "react.js"
  ],
  "bin": {
    "eslint-config-prettier-check": "cli.js"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "prettier"
  ],
  "scripts": {
    "test-cli-sanity": "eslint --print-config .eslintrc.js | ./cli.js",
    "test-lint": "eslint .",
    "test": "npm run test-lint && npm run test-cli-sanity"
  },
  "dependencies": {
    "get-stdin": "^5.0.1"
  },
  "devDependencies": {
    "eslint": ">=3.14.1",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-prettier": "^2.0.0",
    "eslint-plugin-react": "^6.9.0",
    "prettier": "^0.15.0"
  },
  "peerDependencies": {
    "eslint": ">=3.14.1"
  }
}
